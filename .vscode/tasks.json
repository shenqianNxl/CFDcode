{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Build CFDcode (all sources)",
      "command": "D:\\msys64\\ucrt64\\bin\\g++.exe",
      "args": [
        "-std=c++17",
        "-O2",
        "-Wall",
        "-Wextra",
        "main.cpp",
        "src\\math_utils.cpp",
        "src\\RK3_WENO_Euler2d.cpp",
        "src\\FluxFunctions_Euler2d.cpp",
        "src\\Fluxsplit.cpp",
        "src\\TreatBCs_Euler2d.cpp",
        "src\\WENO_Flux.cpp",
        "-o",
        "${workspaceFolder}\\CFDcode.exe"
      ],
      "options": { "cwd": "${workspaceFolder}" },
      "problemMatcher": ["$gcc"],
      "group": { "kind": "build", "isDefault": true }
    }
  ]
}